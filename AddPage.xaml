<Window x:Class="test_avto.AddPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:test_avto"
        mc:Ignorable="d"
        Title="AddPage" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <ComboBox Name="pacAdd" SelectedItem="{Binding PacientIdPacientNavigation}" DisplayMemberPath="PacientFamiliya"/>
            <ComboBox Name="vrAdd" SelectedItem="{Binding VrachIdVrachNavigation}" DisplayMemberPath="VrachFamiliya"/>
            <TextBox Text="{Binding DateOfPriem}"/>
            <Button Content="Add" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
